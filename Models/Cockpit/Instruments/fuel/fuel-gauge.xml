<?xml version="1.0"?>

<PropertyList>

<path>fuel-gauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>Fuelgauge.001</object-name>
        <object-name>RFneedle</object-name>
        <object-name>CFneedle</object-name>
        <object-name>FFneedle</object-name>
        <emission>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.35</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FFneedle</object-name>
        <property>/consumables/fuel/tank/level-gal_us</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>6</ind><dep>29</dep></entry>
            <entry><ind>11</ind><dep>59</dep></entry>
            <entry><ind>22</ind><dep>119</dep></entry>
            <entry><ind>33</ind><dep>180</dep></entry>
            <entry><ind>45</ind><dep>242</dep></entry>
            <entry><ind>50</ind><dep>272</dep></entry>
            <entry><ind>56</ind><dep>300</dep></entry>
        </interpolation>
            <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.012</y-m>
            <z-m>-0.007</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>CFneedle</object-name>
        <property>/consumables/fuel/tank[1]/level-gal_us</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>6</ind><dep>29</dep></entry>
            <entry><ind>11</ind><dep>59</dep></entry>
            <entry><ind>22</ind><dep>119</dep></entry>
            <entry><ind>33</ind><dep>180</dep></entry>
            <entry><ind>45</ind><dep>242</dep></entry>
            <entry><ind>50</ind><dep>272</dep></entry>
            <entry><ind>56</ind><dep>300</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0.015</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RFneedle</object-name>
        <property>/consumables/fuel/tank[2]/level-gal_us</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>4</ind><dep>29</dep></entry>
            <entry><ind>8</ind><dep>59</dep></entry>
            <entry><ind>15</ind><dep>119</dep></entry>
            <entry><ind>22</ind><dep>180</dep></entry>
            <entry><ind>30</ind><dep>242</dep></entry>
            <entry><ind>33</ind><dep>272</dep></entry>
            <entry><ind>37</ind><dep>300</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m> 0 </x-m>
            <y-m> 0.012</y-m>
            <z-m>-0.007</z-m>
        </center>
    </animation>

</PropertyList>
