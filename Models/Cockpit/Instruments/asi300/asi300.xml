<?xml version="1.0"?>
<PropertyList>

    <path>asi300gauge.ac</path>
    <animation>
        <type>material</type>
        <object-name>ASIbezel</object-name>
        <object-name>speed.set</object-name>
        <emission>
            <red>0.005</red>
            <green>0.005</green>
            <blue>0.005</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>ASIface</object-name>
        <object-name>ASIbezel</object-name>
        <object-name>ASI.needle</object-name>
        <object-name>ASI.needle.001</object-name>
        <object-name>index.pointer1</object-name>
        <object-name>index.pointer2</object-name>
        <emission>
            <red>0.035</red>
            <green>0.035</green>
            <blue>0.035</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ASI.needle.001</object-name>
        <property>/autopilot/settings/target-speed-kt</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>40.0</ind><dep>10.0</dep></entry>
            <entry><ind>100.0</ind><dep>88.0</dep></entry>
            <entry><ind>170.0</ind><dep>178.0</dep></entry>
            <entry><ind>240.0</ind><dep>268.0</dep></entry>
            <entry><ind>300.0</ind><dep>333.0</dep></entry>
            <entry><ind>400.0</ind><dep>359.9</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ASI.needle</object-name>
        <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>40.0</ind><dep>10.0</dep></entry>
            <entry><ind>100.0</ind><dep>88.0</dep></entry>
            <entry><ind>170.0</ind><dep>178.0</dep></entry>
            <entry><ind>240.0</ind><dep>268.0</dep></entry>
            <entry><ind>300.0</ind><dep>333.0</dep></entry>
            <entry><ind>400.0</ind><dep>359.9</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>index.pointer1</object-name>
        <property>/instrumentation/airspeed-indicator/index[0]</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>30.0</ind><dep>0.0</dep></entry>
            <entry><ind>40.0</ind><dep>10.0</dep></entry>
            <entry><ind>100.0</ind><dep>88.0</dep></entry>
            <entry><ind>170.0</ind><dep>178.0</dep></entry>
            <entry><ind>240.0</ind><dep>268.0</dep></entry>
            <entry><ind>300.0</ind><dep>333.0</dep></entry>
            <entry><ind>400.0</ind><dep>359.9</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>index.pointer2</object-name>
        <property>/instrumentation/airspeed-indicator/index[1]</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>30.0</ind><dep>0.0</dep></entry>
            <entry><ind>40.0</ind><dep>10.0</dep></entry>
            <entry><ind>100.0</ind><dep>88.0</dep></entry>
            <entry><ind>170.0</ind><dep>178.0</dep></entry>
            <entry><ind>240.0</ind><dep>268.0</dep></entry>
            <entry><ind>300.0</ind><dep>333.0</dep></entry>
            <entry><ind>400.0</ind><dep>359.9</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>

<!--  speed.set control -->
  <animation>
    <type>rotate</type>
    <object-name>speed.set</object-name>
    <property>/autopilot/settings/target-speed-kt</property>
    <factor>5</factor>
    <center>
      <x-m> 4.3603</x-m>
      <y-m> 0.0296</y-m>
      <z-m>-0.0288</z-m>
    </center>
    <axis>
      <x>-1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>speed.plane</object-name>
    <visible>false</visible>
    <action>
      <name>small increase (scrollwheel)</name>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>5</step>
        <min>0</min>
        <max>400</max>
        <wrap>1</wrap>
      </binding>
    </action>
    <action>
      <name>small decrease (scrollwheel)</name>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>-5</step>
        <min>0</min>
        <max>400</max>
        <wrap>1</wrap>
      </binding>
    </action>
    <action>
      <name>increase</name>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>5</step>
        <min>0</min>
        <max>400</max>
        <wrap>1</wrap>
      </binding>
    </action>
    <action>
      <name>decrease</name>
      <button>1</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/autopilot/settings/target-speed-kt</property>
        <step>-5</step>
        <min>0</min>
        <max>400</max>
        <wrap>1</wrap>
      </binding>
    </action>
  </animation>

</PropertyList>
